{
    "id": "tomcat7-maven-clickstart",
    "name": "Tomcat 7",
    "source": "git://github.com/CloudBees-community/tomcat7-maven-clickstart.git",
    "description" : "Deploy your Maven web application on Tomcat 7",
    "doc-url" : "https://github.com/CloudBees-community/tomcat7-maven-clickstart/blob/master/README.md",
    "app-package": "https://repository-community.forge.cloudbees.com/release/localdomain/localhost/tomcat7-maven-clickstart/1.0.1/tomcat7-maven-clickstart-1.0.1.war",
    "icon" : "https://raw.github.com/CloudBees-community/tomcat7-maven-clickstart/master/icon.png",

    "supported-by" : "cloudbees",
    "order" : 1,

    "runtime": {
        "type": "tomcat7"
    },
    "databases": [
        {
            "type": "mysql",
            "alias": "mydb"
        }
    ],
    "build-with-jenkins": {
        "template": {
            "type": "maven"
        }
    }
}
